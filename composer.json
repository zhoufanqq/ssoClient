{
    "name": "zhoufanqq/sso-client",
    "type": "library",
    "description": "单点登录客户端（laravel版）",
    "keywords": [
        "zhoufanqq",
        "ssoClient"
    ],
    "homepage": "https://github.com/zhoufanqq/ssoClient",
    "license": "MIT",
    "authors": [
        {
            "name": ":zhoufan",
            "email": "zhoufanqq@126.com",
            "homepage": "http://zhoufanqq.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {

        "psr-4": {
            "zhoufanqq\\ssoClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "zhoufanqq\\ssoClient\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
